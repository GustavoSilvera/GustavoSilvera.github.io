<script>
  function easter() {
      document.getElementById("rainbowEgg").setAttribute("style","opacity: 1;");
      var elements = document.getElementsByClassName("rainbowEggTXT");
      for(var i=0; i<elements.length; i++) {
	  elements[i].setAttribute("style", "-webkit-animation: rgbTXT 20s infinite;");
	  elements[i].setAttribute("style", "animation: rgbTXT 20s infinite;");
      }
  }
</script>

